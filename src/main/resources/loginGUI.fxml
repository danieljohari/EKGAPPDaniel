<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginGUIController">
   <children>
      <Button fx:id="loginKnap" layoutX="251.0" layoutY="277.0" mnemonicParsing="false" onMouseClicked="#login" text="Login" />
      <Label fx:id="labelLæge" layoutX="117.0" layoutY="73.0" prefHeight="47.0" prefWidth="464.0" text="DIAGNOMONIC LOGIN" textFill="#0691c7">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="200.0" layoutY="155.0" promptText="Username" />
      <PasswordField fx:id="txtPassword" layoutX="200.0" layoutY="200.0" promptText="Password" />
      <ImageView fx:id="LOGO" fitHeight="86.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@LOGOHVID'.JPG" />
         </image>
      </ImageView>
      <ToggleButton fx:id="lægeKnap" layoutX="154.0" layoutY="236.0" mnemonicParsing="false" onMouseClicked="#lKnap" text="Læge" />
      <ToggleButton fx:id="sygeKnap" layoutX="230.0" layoutY="236.0" mnemonicParsing="false" text="Sygeplejerske" onMouseClicked="#sKnap" />
      <ToggleButton fx:id="patientKnap" layoutX="349.0" layoutY="236.0" mnemonicParsing="false" text="Patient" onMouseClicked="#pKnap" />
   </children>
</AnchorPane>
